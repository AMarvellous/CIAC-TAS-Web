

@model adminlte.TASMaterialProgramaAnaliticoService.TASMaterialProgramaAnaliticoEntity

@{
    ViewBag.Title = "TASMaterialProgramaAnaliticoSubirMaterial";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            TAS
            <small>Programa Analitico</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li><a href="#">Programa Analitico</a></li>
            <li class="active">Asignado</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <!-- Default box -->
        <div class="box box-primary">

            <div class="box-header with-border">
                <h3 class="box-title">CIAC TAS</h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            @*<form role="form">*@
            <form action="@Url.Action("TASMaterialProgramaAnaliticoSubirMaterial","TASProgramaAnalitico")" method="post" role="form" enctype="multipart/form-data">
                <div class="box-body">

                    <div class="form-group">
                        @Html.Label("Programa")
                        <div class="input-group">
                            <div class="input-group-addon">
                                <i class="fa fa-users"></i>
                            </div>
                            @Html.TextBoxFor(model => model.Programa, new { @class = "form-control", @readonly = "readonly" })
                            @Html.ValidationMessageFor(model => model.Programa, "", new { @class = "text-danger" })
                            @*<input type="number" class="form-control">*@
                        </div>
                        <!-- /.input group -->
                    </div>

                    <div class="form-group">
                        @Html.Label("Grupo")
                        <div class="input-group">
                            <div class="input-group-addon">
                                <i class="fa fa-users"></i>
                            </div>
                            @Html.TextBoxFor(model => model.Grupo, new { @class = "form-control", @readonly = "readonly" })
                            @Html.ValidationMessageFor(model => model.Grupo, "", new { @class = "text-danger" })
                            @*<input type="number" class="form-control">*@
                        </div>
                        <!-- /.input group -->
                    </div>

                    <div class="form-group">
                        @Html.Label("Materia")
                        <div class="input-group">
                            <div class="input-group-addon">
                                <i class="fa fa-users"></i>
                            </div>
                            @Html.TextBoxFor(model => model.Materia, new { @class = "form-control", @readonly = "readonly" })
                            @Html.ValidationMessageFor(model => model.Materia, "", new { @class = "text-danger" })
                            @*<input type="number" class="form-control">*@
                        </div>
                        <!-- /.input group -->
                    </div>

                    <div class="form-group">
                        @Html.Label("ModuloMateria")
                        <div class="input-group">
                            <div class="input-group-addon">
                                <i class="fa fa-users"></i>
                            </div>
                            @Html.TextBoxFor(model => model.ModuloMateria, new { @class = "form-control", @readonly = "readonly" })
                            @Html.ValidationMessageFor(model => model.ModuloMateria, "", new { @class = "text-danger" })
                            @*<input type="number" class="form-control">*@
                        </div>
                        <!-- /.input group -->
                    </div>

                    <div class="form-group">
                        @Html.Label("Gestion")
                        <div class="input-group">
                            <div class="input-group-addon">
                                <i class="fa fa-users"></i>
                            </div>
                            @Html.TextBoxFor(model => model.Gestion, new { @class = "form-control", @readonly = "readonly" })
                            @Html.ValidationMessageFor(model => model.Gestion, "", new { @class = "text-danger" })
                            @*<input type="number" class="form-control">*@
                        </div>
                        <!-- /.input group -->
                    </div>

                    <div class="form-group">
                        @Html.Label("InstructorNroLicencia")
                        <div class="input-group">
                            <div class="input-group-addon">
                                <i class="fa fa-users"></i>
                            </div>
                            @Html.TextBoxFor(model => model.InstructorNroLicencia, new { @class = "form-control", @readonly = "readonly" })
                            @Html.ValidationMessageFor(model => model.InstructorNroLicencia, "", new { @class = "text-danger" })
                            @*<input type="number" class="form-control">*@
                        </div>
                        <!-- /.input group -->
                    </div>

                    <div class="form-group">
                        @Html.Label("Archivos")
                        <input type="file" name="FileUpload" id="exampleInputFile" multiple="multiple">
                        <p class="help-block">Seleccione los documentos a subir</p>
                    </div>

                </div>
                <!-- /.box-body -->
                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Subir Archivos</button>
                </div>

            </form>

        </div>
        <!-- /.box -->
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
